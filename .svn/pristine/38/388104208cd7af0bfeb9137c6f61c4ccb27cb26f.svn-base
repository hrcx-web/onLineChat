import request from '@/utils/request'

// vip分页查询
export function vipQuery(params) {
  return request({
    url: '/chat/vip/query',
    method: 'GET',
    params
  })
}
// 批量删除
export function deleteBatch(params) {
  return request({
    url: '/chat/vip/deleteBatch',
    method: 'DELETE',
    params
  })
}
// 添加

export function Add(data) {
  return request({
    url: '/chat/vip/add',
    method: 'POST',
    data
  })
}
// 编辑
export function dir(data) {
  return request({
    url: '/chat/vip/edit',
    method: 'PUT',
    data
  })
}
